import numpy as np
arr1 = np.array([100,200,30])
arr2 = np.array([40,50,60])
arr3 = np.array([[100,200,30],[40,50,60]])
arr4 = np.array([[50,33,44],[55,66,77]])
arr1_2 = np.concatenate((arr1,arr2))
print(arr1_2)
arr3_4 = np.concatenate((arr3,arr4),axis=1)#axis = 0
print(arr3_4)
arr3_4b = np.vstack((arr3,arr4))
print(arr3_4b)
arr3_4c = np.hstack((arr3,arr4))
print(arr3_4c)

